@model List<Agent>

<h2>Liste des Agents</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Numéro d'Identification</th>
            <th>Région</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var agent in Model)
        {
            <tr>
                <td>@agent.Name</td>
                <td>@agent.IdentificationNumber</td>
                <td>@agent.Region</td>
            </tr>
        }
    </tbody>
</table>
